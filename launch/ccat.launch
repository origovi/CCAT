<launch>
  <node name="ccat" pkg="ccat" type="ccat_exec" output="screen">
    <rosparam command="load" file="$(find ccat)/config/ccat.yml" />
    <rosparam command="load" file="$(find ccat)/config/params/extrinsics_left.yml" ns="/extrinsics/left" />
    <rosparam command="load" file="$(find ccat)/config/params/extrinsics_right.yml" ns="/extrinsics/right" />
    <rosparam command="load" file="$(find ccat)/config/params/intrinsics_left.yml" ns="/intrinsics/left" />
    <rosparam command="load" file="$(find ccat)/config/params/intrinsics_right.yml" ns="/intrinsics/right" />
  </node>
</launch>